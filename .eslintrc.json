{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "globals": {
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "warn",
    "prettier/prettier": [
      "error", {
        "arrowParens": "always",
        "printWidth": 120,
        "semi": false,
        "singleQuote": true
      }
    ]
  }
}
